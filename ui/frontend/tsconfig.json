{
  "exclude": [
    "build",
  ],
  "compilerOptions": {
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "target": "ES2017",
    "module": "esnext",
    "sourceMap": true,
    "jsx": "preserve",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
  }
}
